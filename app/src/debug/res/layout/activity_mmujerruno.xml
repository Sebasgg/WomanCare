<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".Mmujerruno">
    <Button
        android:id="@+id/regresar"
        android:layout_width="wrap_content"
        android:backgroundTint="#eed4bc"
        android:text="@string/texto65"
        android:textColor="#6E3500"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints">

    </Button>
    <TextView
        android:id="@+id/logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="48dp"
        android:text="Malala Yousafzaird"
        android:textAlignment="center"
        android:textColor="#AEE449E4"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="404dp"
        android:layout_height="182dp"
        android:layout_gravity="center"

        android:layout_marginTop="108dp"
        android:src="@drawable/mujerruno"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="3dp" />

    <LinearLayout
        android:layout_width="400dp"
        android:layout_height="456dp"

        android:background="#1BF18EB0"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.871"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="3dp">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:justificationMode="inter_word"
            android:text="@string/texto80" />

        <LinearLayout
            android:layout_width="200dp"
            android:layout_height="15dp"></LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:justificationMode="inter_word"
            android:text="@string/texto81" />

        <LinearLayout
            android:layout_width="200dp"
            android:layout_height="15dp"></LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:justificationMode="inter_word"
            android:text="@string/texto82">

        </TextView>


    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>